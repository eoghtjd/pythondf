import numpy as np

#1
nar = np.full((10), 0)
print(nar)
#2
nar[4] = 1
print(nar)
#3
nar2=np.arange(10,31)
print(nar2)
#4
print(np.random.randint(99,size=(2,2)))
#5
print(np.random.rand(2,4))

#6
nar3=np.arange(35,75)
print(nar3)
nar4=nar3.reshape(10,4)
print(nar4)

#7 맨끝의행부터 역순으로 출력


print(nar4[::-1])

#8 2번쨰행부터 마지막 직전행까ㅣ지 세번쨰 열부터 마지막 열까찌 슬라이싱
print('8888')
print(nar4[1:-1,2:4])

#9 
print(nar4[1:-1,3:3].flatten())